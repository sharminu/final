    <script type="text/javascript">
        if (self === top || allowDomains('app.optimizely.com')) {
            var antiClickjack = document.getElementById("antiClickjack");
            antiClickjack.parentNode.removeChild(antiClickjack);
        } else {
            top.location = self.location;
        }
        
        function allowDomains(xFrameAllowDomains){
            var url = (window.location != window.parent.location)
                       ? document.referrer : document.location;
            var domains = xFrameAllowDomains.split(" ");
            for (index in domains) {
                if(url.match('/' + domains[index] + '/')){
                    return true;
                }
            }
            return false;
        }
        
        
        window.handleOrderSummary = function() {

            try {
                var orderService = angular.element(document.querySelector('.ph-content-view')).injector().get('OrderService'),
                    orderSummary = orderService.getCachedSummaryData(),
                    request = {
                        summary : orderSummary.summary,
                        items : orderSummary.items
                    };
            } catch(e) {
                console.error("Error: sendOrderSummary failed");
                console.error(e);
            }
            return JSON.stringify(request);
         };

         window.handleTokenCC = function(token) {
             
              console.log("handleTokenCC with token : " + token);
             
             if(!token){
                  console.log("EmptyhandleToken");
                 return;
             }
             
             if(typeof token === 'string') {
                 token = JSON.parse(token);
             }
             
             if(token.event) {
                token = token.event;
             }

             var orderService = angular.element(document.querySelector('.ph-content-view')).injector().get('OrderService');
             orderService.sendApplePayToken(token);

         };
         
         //Function to open signinmodal.
         window.loyaltyLogin = function() {
            var accountService = angular.element(document.querySelector('.ph-content-view')).injector().get('AccountService');
            accountService.data.loyaltyIframeFlowflag = true;
            accountService.loyaltySignIn();         
         };
             
         //Function to send the serialized code.
         window.sendLoyaltySerializedCode = function(loyaltyRedeem) {
              var dealsService = angular.element(document.querySelector('.ph-content-view')).injector().get('DealsService');
                dealsService.setLoyaltySerializedCode(loyaltyRedeem);
         };
         
         //Function to enable loyalty for user
         //returns token for user.
         window.toggleLoyalty = function (toggle) {
             var accountService = angular.element(document.querySelector('.ph-content-view')).injector().get('AccountService');
              accountService.toggleUser(toggle);
            };

         function receiveMessage(event) { 
                // Check to make sure that this message came from the correct domain.                         
                // Array of acceptable domains to receive calls from.
                var allowed_domains = [
                        'https://hutrewards.pizzahut.com', // Prod
                        'https://staging-hutrewards.pizzahut.com' // Stage
                ];
                 
                if ( allowed_domains.indexOf(event.origin) === -1) {
                        // unsafe origin url, do nothing.
                        return;
                } else {
                     var accountService = angular.element(document.querySelector('.ph-content-view')).injector().get('AccountService');
                    // safe origin url.
                    if (event && event.data === "Sign In") {
                        loyaltyLogin();
                    } else if (event && event.data && event.data.loyalty_toggle === 'on') {
                        toggleLoyalty(true);
                    } else if (event && event.data && event.data.loyalty_toggle === 'off') {
                        toggleLoyalty(false);
                    } else if (event && event.data && (event.data.height || event.data.scroll || event.data.modal)) {
                        accountService.iframeMessage(event);                               
                    } else if (event && event.data && event.data.coupon) {
                        sendLoyaltySerializedCode(event.data.coupon);
                    }else if(event && event.data && event.data.create_new_user) {
                        accountService.updateHrAccount(event.data.create_new_user);
                    }else if(event && event.data === "Create Account") {
                        accountService.goToHrCreateAccount();
                    }
                }
        }

        // Setup an event listener that calls receiveMessage() when the window
        // receives a new MessageEvent.
        window.addEventListener('message', receiveMessage);

        
    </script>
